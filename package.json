{
  "name": "sierra",
  "version": "0.4.0",
  "description": "A service framework for Node.js.",
  "repository": "https://www.github.com/sjohnsonaz/sierra.git",
  "license": "MIT",
  "author": "Sean Johnson <sjohnson@sjohnsonaz.com>",
  "main": "dist/scripts/Sierra.js",
  "typings": "dist/scripts/Sierra.d.ts",
  "scripts": {
    "start": "node ./dist/scripts/Sierra.js",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "prepublishOnly": "rimraf ./dist && tsc --declaration",
    "test": "tsc && node src/mocha/NodeRunner.js",
    "watch": "tsc --watch",
    "watch-link": "tsc --declaration --watch",
    "node": "node --inspect ./dist/testApplications/SierraTest.js",
    "nodemon": "nodemon ./dist/testApplications/SierraTest.js",
    "mkdocs": "mkdocs build",
    "mkdocs-serve": "mkdocs serve",
    "mkdocs-deploy": "mkdocs gh-deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/handlebars": "4.1.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.5",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "handlebars": "4.7.3",
    "mocha": "7.1.1",
    "nodemon": "2.0.2",
    "rimraf": "3.0.2",
    "typescript": "3.8.3"
  },
  "keywords": [
    "service",
    "framework",
    "node",
    "middleware",
    "routing",
    "router"
  ]
}
